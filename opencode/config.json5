{
  "$schema": "https://opencode.ai/config.json",
  "plugin": ["opencode-antigravity-auth@1.1.2", "opencode-openai-codex-auth@4.2.0"],
  "provider": {
    "google": {
      "name": "Antigravity",
      "api": "antigravity",
      "models": {
        "gemini-3-pro-high": {
          "name": "Gemini 3 Pro (High)",
          "options": {
            "thinking_level": "high"
          }
        },
        "gemini-3-pro-medium": {
          "name": "Gemini 3 Pro (Medium)",
          "options": {
            "thinking_level": "medium"
          }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash",
          "limit": {
            "context": 1000000,
            "output": 8192
          }
        }
      }
    },
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": ["reasoning.encrypted_content"],
        "store": false
      },
      "models": {
        "gpt-5.2-high": {
          "name": "GPT 5.2 High (OAuth)",
          "limit": { "context": 272000, "output": 128000 },
          "modalities": { "input": ["text", "image"], "output": ["text"] },
          "options": {
            "reasoningEffort": "high",
            "reasoningSummary": "detailed",
            "textVerbosity": "medium",
            "include": ["reasoning.encrypted_content"],
            "store": false
          }
        },
        "gpt-5.2-codex-max-high": {
          "name": "GPT 5.2 Codex Max High (OAuth)",
          "limit": { "context": 272000, "output": 128000 },
          "modalities": { "input": ["text", "image"], "output": ["text"] },
          "options": {
            "reasoningEffort": "high",
            "reasoningSummary": "detailed",
            "textVerbosity": "medium",
            "include": ["reasoning.encrypted_content"],
            "store": false
          }
        }
      }
    }
  },
  "failover": {
    "opus-4.5-failover": {
      "name": "Claude Opus 4.5 (Failover)",
      "chain": [
        { "provider": "anthropic", "model": "claude-opus-4-5" },
        { "provider": "github-copilot", "model": "claude-opus-4.5" }
      ]
    },
    "gpt-5.2-codex-failover": {
      "name": "GPT 5.2 Codex (Failover)",
      "chain": [
        { "provider": "openai", "model": "gpt-5.2-codex-max-high" },
        { "provider": "github-copilot", "model": "gpt-5.2" }
      ]
    }
  }
}
