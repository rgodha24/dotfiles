{
  "$schema": "https://opencode.ai/config.json",
  "plugin": ["opencode-antigravity-auth@1.1.2", "opencode-openai-codex-auth"],
  "provider": {
    "google": {
      "name": "Antigravity",
      "api": "antigravity",
      "models": {
        "gemini-3-pro-high": {
          "name": "Gemini 3 Pro (High)",
          "options": {
            "thinking_level": "high",
          },
        },
        "gemini-3-pro-medium": {
          "name": "Gemini 3 Pro (Medium)",
          "options": {
            "thinking_level": "medium",
          },
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash",
          "limit": {
            "context": 1000000,
            "output": 8192,
          },
        },
      },
    },
  },
}
